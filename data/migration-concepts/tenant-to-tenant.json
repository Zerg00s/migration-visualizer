{
  "id": "tenant-to-tenant",
  "name": "Tenant-to-Tenant Migration",
  "description": "Microsoft 365 tenant-to-tenant migration visualization",
  "dataSource": "data/tenant-to-tenant-data.json",
  "objectTypes": {
    "user": {
      "icon": "fa-user",
      "color": "#64b5f6",
      "dataProperties": {
        "collection": "Users",
        "idField": "id",
        "nameField": "DisplayName",
        "additionalFields": ["Title", "Department", "Email", "Disabled", "LastLogin", "MFAEnabled", "LicenseType"]
      }
    },
    "group": {
      "icon": "fa-users", 
      "color": "#7e57c2",
      "dataProperties": {
        "collection": "SecurityGroups",
        "idField": "id",
        "nameField": "DisplayName",
        "additionalFields": ["Description", "Members", "CreatedDate", "Classification"]
      }
    },
    "m365-group": {
      "icon": "fa-users-cog",
      "color": "#ff9800", 
      "dataProperties": {
        "collection": "M365Groups",
        "idField": "id",
        "nameField": "DisplayName",
        "additionalFields": ["Email", "Description", "Owners", "Members", "Visibility"]
      }
    },
    "sharepoint": {
      "icon": "fa-share-alt",
      "color": "#4caf50",
      "dataProperties": {
        "collection": "SharePointSites", 
        "idField": "id",
        "nameField": "DisplayName",
        "additionalFields": ["URL", "Owners", "Members", "StorageUsed"]
      }
    },
    "teams": {
      "icon": "fa-comments",
      "color": "#673ab7",
      "dataProperties": {
        "collection": "Teams",
        "idField": "id", 
        "nameField": "DisplayName",
        "additionalFields": ["Description", "Owners", "Members", "Channels"]
      }
    },
    "onedrive": {
      "icon": "fa-cloud",
      "color": "#2196f3",
      "dataProperties": {
        "collection": "OneDrives",
        "idField": "id",
        "nameField": "DisplayName", 
        "additionalFields": ["OwnedBy", "StorageUsed", "SharedWith"]
      }
    },
    "mailbox": {
      "icon": "fa-envelope",
      "color": "#e91e63",
      "dataProperties": {
        "collection": "Mailboxes",
        "idField": "id",
        "nameField": "DisplayName",
        "additionalFields": ["Email", "OwnedBy", "StorageUsed", "ItemCount"]
      }
    },
    "power-automate": {
      "icon": "fa-bolt", 
      "color": "#0066ff",
      "dataProperties": {
        "collection": "PowerAutomate",
        "idField": "id",
        "nameField": "DisplayName",
        "additionalFields": ["OwnedBy", "State", "Runs", "Category"]
      }
    },
    "power-apps": {
      "icon": "fa-mobile-alt",
      "color": "#742774",
      "dataProperties": {
        "collection": "PowerApps", 
        "idField": "id",
        "nameField": "DisplayName",
        "additionalFields": ["OwnedBy", "AppType", "Users", "Category"]
      }
    },
    "power-bi": {
      "icon": "fa-chart-bar",
      "color": "#f2c811",
      "dataProperties": {
        "collection": "PowerBI",
        "idField": "id",
        "nameField": "DisplayName", 
        "additionalFields": ["OwnedBy", "WorkspaceName", "Viewers", "RefreshSchedule"]
      }
    }
  },
  "sourceEnvironment": {
    "title": "Source Tenant",
    "buckets": [
      {"id": "source-users", "title": "Entra ID Users", "objectType": "user"},
      {"id": "source-security-groups", "title": "Entra ID Security Groups", "objectType": "group"},
      {"id": "source-sharepoint", "title": "SharePoint Online", "objectType": "sharepoint"},
      {"id": "source-teams", "title": "Teams", "objectType": "teams"},
      {"id": "source-onedrive", "title": "OneDrive", "objectType": "onedrive"},
      {"id": "source-m365-groups", "title": "M365 Groups", "objectType": "m365-group"},
      {"id": "source-mailboxes", "title": "Mailboxes", "objectType": "mailbox"},
      {"id": "source-power-automate", "title": "Power Automate", "objectType": "power-automate"},
      {"id": "source-power-apps", "title": "Power Apps", "objectType": "power-apps"},
      {"id": "source-power-bi", "title": "Power BI", "objectType": "power-bi"}
    ]
  },
  "targetEnvironment": {
    "title": "Destination Tenant", 
    "buckets": [
      {"id": "destination-users", "title": "Entra ID Users", "objectType": "user"},
      {"id": "destination-security-groups", "title": "Entra ID Security Groups", "objectType": "group"},
      {"id": "destination-sharepoint", "title": "SharePoint Online", "objectType": "sharepoint"},
      {"id": "destination-teams", "title": "Teams", "objectType": "teams"},
      {"id": "destination-onedrive", "title": "OneDrive", "objectType": "onedrive"},
      {"id": "destination-m365-groups", "title": "M365 Groups", "objectType": "m365-group"},
      {"id": "destination-mailboxes", "title": "Mailboxes", "objectType": "mailbox"},
      {"id": "destination-power-automate", "title": "Power Automate", "objectType": "power-automate"},
      {"id": "destination-power-apps", "title": "Power Apps", "objectType": "power-apps"},
      {"id": "destination-power-bi", "title": "Power BI", "objectType": "power-bi"}
    ]
  },
  "connectionRules": [
    {
      "name": "User owns Mailbox",
      "type": "ownership",
      "from": {"objectType": "user"},
      "to": {"objectType": "mailbox", "property": "OwnedBy"}
    },
    {
      "name": "User owns OneDrive", 
      "type": "ownership",
      "from": {"objectType": "user"},
      "to": {"objectType": "onedrive", "property": "OwnedBy"}
    },
    {
      "name": "User is Group Member",
      "type": "membership", 
      "from": {"objectType": "user"},
      "to": {"objectType": "group", "arrayProperty": "Members"}
    },
    {
      "name": "User owns SharePoint Site",
      "type": "site-owner",
      "from": {"objectType": "user"},
      "to": {"objectType": "sharepoint", "arrayProperty": "Owners"}
    },
    {
      "name": "User owns Power Platform",
      "type": "ownership",
      "from": {"objectType": "user"},
      "to": {"objectType": "power-automate", "property": "OwnedBy"}
    },
    {
      "name": "User owns Power Apps",
      "type": "ownership", 
      "from": {"objectType": "user"},
      "to": {"objectType": "power-apps", "property": "OwnedBy"}
    },
    {
      "name": "User owns Power BI",
      "type": "ownership",
      "from": {"objectType": "user"}, 
      "to": {"objectType": "power-bi", "property": "OwnedBy"}
    }
  ],
  "mappings": [
    {"sourceType": "user", "targetType": "user", "targetBucket": "destination-users"},
    {"sourceType": "group", "targetType": "group", "targetBucket": "destination-security-groups"},
    {"sourceType": "m365-group", "targetType": "m365-group", "targetBucket": "destination-m365-groups"},
    {"sourceType": "sharepoint", "targetType": "sharepoint", "targetBucket": "destination-sharepoint"},
    {"sourceType": "teams", "targetType": "teams", "targetBucket": "destination-teams"},
    {"sourceType": "onedrive", "targetType": "onedrive", "targetBucket": "destination-onedrive"},
    {"sourceType": "mailbox", "targetType": "mailbox", "targetBucket": "destination-mailboxes"},
    {"sourceType": "power-automate", "targetType": "power-automate", "targetBucket": "destination-power-automate"},
    {"sourceType": "power-apps", "targetType": "power-apps", "targetBucket": "destination-power-apps"},
    {"sourceType": "power-bi", "targetType": "power-bi", "targetBucket": "destination-power-bi"}
  ]
}