{
  "id": "google-workspace",
  "name": "Google Workspace to Microsoft 365",
  "description": "Google Workspace to Microsoft 365 migration with email and drive transformation",
  "dataSource": "data/google-workspace-data.json",
  "sourceEnvironment": {
    "name": "source",
    "title": "Google Workspace",
    "description": "Google Workspace environment",
    "buckets": [
      {
        "id": "source-google-users",
        "title": "Google Users",
        "objectType": "google-user",
        "layout": { "gridColumn": 1, "gridRow": 1 }
      },
      {
        "id": "source-google-groups",
        "title": "Google Groups",
        "objectType": "google-group",
        "layout": { "gridColumn": 2, "gridRow": 1 }
      },
      {
        "id": "source-google-mailboxes",
        "title": "Gmail",
        "objectType": "gmail",
        "layout": { "gridColumn": 3, "gridRow": 1 }
      },
      {
        "id": "source-google-shared-drives",
        "title": "Shared Drives",
        "objectType": "google-shared-drive",
        "layout": { "gridColumn": 1, "gridRow": 2 }
      },
      {
        "id": "source-google-drives",
        "title": "Google Drive",
        "objectType": "google-drive",
        "layout": { "gridColumn": 2, "gridRow": 2 }
      }
    ]
  },
  "targetEnvironment": {
    "name": "destination",
    "title": "Microsoft 365",
    "description": "Microsoft 365 cloud environment",
    "buckets": [
      {
        "id": "destination-entra-users",
        "title": "Entra ID Users",
        "objectType": "entra-user",
        "layout": { "gridColumn": 1, "gridRow": 1 }
      },
      {
        "id": "destination-entra-groups",
        "title": "Entra ID Security Groups",
        "objectType": "entra-security-group",
        "layout": { "gridColumn": 2, "gridRow": 1 }
      },
      {
        "id": "destination-exchange-online",
        "title": "Exchange Online",
        "objectType": "exchange-mailbox",
        "layout": { "gridColumn": 3, "gridRow": 1 }
      },
      {
        "id": "destination-sharepoint-online",
        "title": "SharePoint Online",
        "objectType": "sharepoint-site",
        "layout": { "gridColumn": 1, "gridRow": 2 }
      },
      {
        "id": "destination-onedrive",
        "title": "OneDrive",
        "objectType": "onedrive",
        "layout": { "gridColumn": 2, "gridRow": 2 }
      }
    ]
  },
  "objectTypes": {
    "google-user": {
      "name": "Google User",
      "pluralName": "Google Users",
      "icon": "fab fa-google",
      "color": "#4285f4",
      "dataProperties": {
        "collection": "GoogleUsers",
        "idField": "id",
        "nameField": "DisplayName",
        "additionalFields": ["Email", "GoogleUsername", "AccountStatus", "TwoFactorEnabled", "GoogleWorkspaceRole"]
      }
    },
    "google-group": {
      "name": "Google Group",
      "pluralName": "Google Groups",
      "icon": "fa-users",
      "color": "#34a853",
      "dataProperties": {
        "collection": "GoogleGroups",
        "idField": "id",
        "nameField": "DisplayName",
        "additionalFields": ["Email", "Members", "GroupType", "WhoCanJoin"]
      }
    },
    "gmail": {
      "name": "Gmail",
      "pluralName": "Gmail Mailboxes",
      "icon": "fa-envelope",
      "color": "#ea4335",
      "dataProperties": {
        "collection": "GoogleMailboxes",
        "idField": "id",
        "nameField": "DisplayName",
        "additionalFields": ["Email", "Owner", "StorageUsed", "MessageCount"]
      }
    },
    "google-shared-drive": {
      "name": "Shared Drive",
      "pluralName": "Shared Drives",
      "icon": "fa-folder-open",
      "color": "#fbbc05",
      "dataProperties": {
        "collection": "GoogleSharedDrives",
        "idField": "id",
        "nameField": "DisplayName",
        "additionalFields": ["StorageUsed", "FileCount", "Organizers", "ContentManagers", "AccessLevel"]
      }
    },
    "google-drive": {
      "name": "Google Drive",
      "pluralName": "Google Drives",
      "icon": "fab fa-google-drive",
      "color": "#ff6d01",
      "dataProperties": {
        "collection": "GoogleDrives",
        "idField": "id",
        "nameField": "DisplayName",
        "additionalFields": ["Owner", "StorageUsed", "FileCount", "SyncEnabled"]
      }
    },
    "entra-user": {
      "name": "Entra ID User",
      "pluralName": "Entra ID Users",
      "icon": "fa-user",
      "color": "#64b5f6",
      "dataProperties": {
        "collection": "GoogleUsers",
        "idField": "id",
        "nameField": "DisplayName",
        "additionalFields": ["Email", "Title", "Department"]
      }
    },
    "entra-security-group": {
      "name": "Entra ID Security Group",
      "pluralName": "Entra ID Security Groups",
      "icon": "fa-users",
      "color": "#7e57c2",
      "dataProperties": {
        "collection": "GoogleGroups",
        "idField": "id",
        "nameField": "DisplayName",
        "additionalFields": ["Description", "Members"]
      }
    },
    "exchange-mailbox": {
      "name": "Exchange Mailbox",
      "pluralName": "Exchange Mailboxes",
      "icon": "fa-envelope",
      "color": "#0078d4",
      "dataProperties": {
        "collection": "GoogleMailboxes",
        "idField": "id",
        "nameField": "DisplayName",
        "additionalFields": ["Email", "StorageUsed", "MessageCount"]
      }
    },
    "sharepoint-site": {
      "name": "SharePoint Site",
      "pluralName": "SharePoint Sites",
      "icon": "fa-share-alt",
      "color": "#4caf50",
      "dataProperties": {
        "collection": "GoogleSharedDrives",
        "idField": "id",
        "nameField": "DisplayName",
        "additionalFields": ["StorageUsed", "FileCount"]
      }
    },
    "onedrive": {
      "name": "OneDrive",
      "pluralName": "OneDrive",
      "icon": "fa-cloud",
      "color": "#2196f3",
      "dataProperties": {
        "collection": "GoogleDrives",
        "idField": "id",
        "nameField": "DisplayName",
        "additionalFields": ["Owner", "StorageUsed", "FileCount"]
      }
    }
  },
  "mappings": [
    {
      "sourceType": "google-user",
      "targetType": "entra-user",
      "transformations": {
        "icon": "fa-user",
        "color": "#64b5f6"
      }
    },
    {
      "sourceType": "google-group",
      "targetType": "entra-security-group",
      "transformations": {
        "icon": "fa-users",
        "color": "#7e57c2"
      }
    },
    {
      "sourceType": "gmail",
      "targetType": "exchange-mailbox",
      "transformations": {
        "icon": "fa-envelope",
        "color": "#0078d4"
      }
    },
    {
      "sourceType": "google-shared-drive",
      "targetType": "sharepoint-site",
      "transformations": {
        "icon": "fa-share-alt",
        "color": "#4caf50"
      }
    },
    {
      "sourceType": "google-drive",
      "targetType": "onedrive",
      "transformations": {
        "icon": "fa-cloud",
        "color": "#2196f3"
      }
    }
  ],
  "connectionRules": [
    {
      "type": "ownership",
      "name": "Owns",
      "from": {
        "objectType": "google-user"
      },
      "to": {
        "objectType": "gmail",
        "property": "Owner"
      }
    },
    {
      "type": "ownership",
      "name": "Owns",
      "from": {
        "objectType": "google-user"
      },
      "to": {
        "objectType": "google-drive",
        "property": "Owner"
      }
    },
    {
      "type": "membership",
      "name": "Member of",
      "from": {
        "objectType": "google-user"
      },
      "to": {
        "objectType": "google-group",
        "arrayProperty": "Members"
      }
    },
    {
      "type": "organizer",
      "name": "Organizer",
      "from": {
        "objectType": "google-user"
      },
      "to": {
        "objectType": "google-shared-drive",
        "arrayProperty": "Organizers"
      }
    },
    {
      "type": "content-manager",
      "name": "Content Manager",
      "from": {
        "objectType": "google-user"
      },
      "to": {
        "objectType": "google-shared-drive",
        "arrayProperty": "ContentManagers"
      }
    }
  ]
}
