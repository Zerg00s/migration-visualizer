    {
      "sourceType": "dropbox-paper",
      "targetType": "onenote",
      "transformations": {
        "icon": "fas fa-book",
        "color": "#7719aa"
      }
    }
  ],
  
  "connectionRules": [
    {
      "type": "ownership",
